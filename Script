-- Buttons/Windows/Idk

Tab2Section:NewToggle("Killaura", "Kill Aura", function(state)

    if state then

        game.Workspace.CurrentCamera.FieldOfView = 120

    else

        game.Workspace.CurrentCamera.FieldOfView = 80

    end

end)

